<h1><%= "User's Request" %></h1>
<div class="row">
  <div class="col-sm-1"></div>
  <div class="col-sm-10">
<%= form_tag admin_requests_path do %>
  <table class="table table-striped">
    <thead>
    <tr>
      <th><%= "user id" %></th>
      <th><%= "title" %></th>
      <th><%= "content" %></th>
      <th><%= "status" %></th>
      <th colspan="3"></th>
    </tr>
    </thead>
    <tbody>
    <%= render "request" %>
    </tbody>
  </table>
<% end %>
  </div>
  <div class="col-sm-1"></div>
</div>
<% will_paginate %>