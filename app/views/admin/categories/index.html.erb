<%= will_paginate @categories %>
<% @categories.each do |category| %>
    <div class="well">
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading"><%= category.name %></h4>
          <ul>
            <%= link_to "Edit", edit_admin_category_path(category) %> |
            <%= link_to "Delete", [:admin, category], method: :delete, data: {confirm: "are you sure?"} %>
          </ul>
        </div>
      </div>
    </div>
    <hr>
<% end %>
<%= will_paginate @categories %>
