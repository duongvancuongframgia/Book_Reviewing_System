<%= will_paginate @reviews %>
<% @reviews.each do |review| %>
  <div class="well">
    <div class="media">
      <div class="media-body">
        <h4 class="media-heading"><%= review.name %></h4>
        <ul>
          <%= link_to t("admin.edit"), edit_admin_review_path(category) %> |
          <%= link_to t("admin.delete"), [:admin, review], method: :delete, data: {confirm: t("admin.are_you_sure")} %>
        </ul>
      </div>
    </div>
  </div>
  <hr>
<% end %>
<%= will_paginate @reviews %>
